{
  "name": "@kuiba/ui",
  "version": "0.0.6",
  "description": "A mobile vue3 components library",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "vetur": {
    "tags": "highlight/tags.json",
    "attributes": "highlight/attributes.json"
  },
  "web-types": "highlight/web-types.json",
  "keywords": [
    "Vue",
    "UI"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "scripts": {
    "husky": "husky install",
    "commit": "git-cz",
    "dev": "kuiba-cli dev",
    "build": "kuiba-cli build",
    "compile": "kuiba-cli compile",
    "create": "kuiba-cli create",
    "lint": "kuiba-cli lint",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "genAllLog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "yarn compile && release-it",
    "test": "kuiba-cli jest",
    "test:watch": "kuiba-cli jest -w"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "release-it": {
    "git": {
      "changelog": "git log --pretty=format:\"* %s (%h)\" ${from}...${to}",
      "tagName": "v${version}",
      "commitMessage": "chore: release ${version}",
      "requireCleanWorkingDir": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue,scss}": "prettier --write",
    "*.{ts,tsx,js,vue}": "eslint --fix",
    "*.{vue,css,scss}": "stylelint --fix"
  },
  "browserslist": [
    "Chrome >= 51",
    "iOS >= 10"
  ],
  "files": [
    "es",
    "umd",
    "highlight",
    "types",
    "README.md"
  ],
  "dependencies": {
    "@kuiba/cli": "^0.0.3",
    "@kuiba/icons": "^0.0.3"
  },
  "gitHead": "b86dcacf701bdc602e3e236b4e0773ffefbe89fc"
}
